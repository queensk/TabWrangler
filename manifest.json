{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Manage and organize too many open tabs in Chrome",
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Manager"
  },
  "icons": {}
}
